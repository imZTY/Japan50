/* pages/note_list/noteList.wxss */
.text-search-back    { 
  background: rgb(248, 248, 248);
}


.single-line{
    display: block;
    color:#212121;
    overflow: hidden;
    white-space: nowrap;
    text-overflow:ellipsis;
    margin: 5rpx 0rpx 3rpx 2rpx;
}

.note-list{
  display: flex;
  flex-direction:column;
  box-sizing: border-box;
  justify-content: space-around;
  margin-top: 30rpx;
  
}

.note-div{
  display: flex;
  flex-direction:column;
  margin: 22rpx;
  padding: 22rpx;
  box-sizing: border-box;
  justify-content: space-around;
  font-style: #757575;
  border-radius: 3%;
}